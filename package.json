{
  "name": "fastmatter",
  "version": "1.0.0",
  "description": "A faster frontmatter parser.",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuanqing/fastmatter"
  },
  "devDependencies": {
    "front-matter": "^1.0.0",
    "glob": "^5.0.6",
    "istanbul": "^0.3.14",
    "jshint": "^2.7.0",
    "jshint-stylish": "^1.0.2",
    "matcha": "^0.6.0",
    "minijasminenode2": "^1.0.0"
  },
  "dependencies": {
    "js-yaml": "^3.3.1",
    "split": "^0.3.3",
    "stream-combiner": "^0.2.1",
    "through2": "^0.6.5"
  },
  "scripts": {
    "bench": "node_modules/.bin/matcha",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/minijasminenode2 spec/*.js -x **/spec/**",
    "lint": "npm run lint-bench && npm run lint-lib && npm run lint-test",
    "lint-bench": "node_modules/.bin/jshint --verbose --reporter node_modules/jshint-stylish/stylish.js ./benchmark/*.js",
    "lint-lib": "node_modules/.bin/jshint --verbose --reporter node_modules/jshint-stylish/stylish.js *.js",
    "lint-test": "node_modules/.bin/jshint --verbose --reporter node_modules/jshint-stylish/stylish.js ./spec/*.js",
    "test": "node_modules/.bin/minijasminenode2 spec/*.js --verbose"
  },
  "keywords": [
    "document",
    "front-matter",
    "frontmatter",
    "markdown",
    "parser",
    "static",
    "text",
    "yaml"
  ]
}
